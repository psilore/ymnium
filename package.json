{
  "name": "ymnium",
  "version": "1.0.0",
  "description": "A Frontend test for Omegapoint",
  "main": "./src/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev npm-run-all --parallel start:app start:server",
    "start:app": "react-scripts start",
    "start:server": "netlify-lambda serve functions",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "netlify-lambda build functions",
    "test": "react-scripts test --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/psilore/ymnium.git"
  },
  "keywords": [
    "frontend"
  ],
  "author": "Erik Emmerfors",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/psilore/ymnium/issues"
  },
  "homepage": "https://gitlab.com/psilore/ymnium#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "humanize-plus": "^1.8.2",
    "morgan": "^1.10.0",
    "omdbapi": "^3.0.0",
    "request": "^2.88.2",
    "serverless-http": "^3.0.1",
    "vue": "^3.2.36",
    "vue-server-renderer": "^2.6.14",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "devDependencies": {
    "aws-serverless-express": "^3.4.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "mini-css-extract-plugin": "^2.6.0",
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5"
  }
}
